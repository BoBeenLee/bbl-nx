  "scripts": {
    "post:new": "yarn plop post",
    "components:new": "yarn plop ui-components",
    "start": "yarn nx serve",
    "dev": "yarn nx serve",
    "build": "yarn nx run-many --target=build --all",
    "build:affected:all": "yarn nx affected --target=build --all",
    "eslint": "yarn nx run-many --target=lint --all",
    "eslint:fix": "yarn nx run-many --target=lint --configuration=fix --all",
    "test": "npm run test:all",
    "test:all": "yarn nx run-many --target=test --all",
    "test:affected": "yarn nx affected --target=test --base=master --head=HEAD",
    "test:affected:ci": "yarn nx affected --target=test",
    "test-storybook": "yarn test-storybook --url http://localhost:4400/",
    "storybook": "npm run ui-components:storybook",
    "build-storybook": "yarn nx run-many --target=build-storybook --projects=ui-components,features,bbl-nextjs",
    "build-storybook:github": "env-cmd -f ./apps/bbl-nextjs/.github.env yarn nx run-many --target=build-storybook --projects=ui-components,features,bbl-nextjs",
    "bbl-nextjs:analyze": "env-cmd -f ./apps/bbl-nextjs/.analyze.env yarn nx run bbl-nextjs:serve",
    "bbl-nextjs:storybook": "yarn nx run bbl-nextjs:storybook",
    "bbl-nextjs:build-storybook": "yarn nx run bbl-nextjs:build-storybook",
    "features:storybook": "yarn nx run features:storybook",
    "features:build-storybook": "yarn nx run features:build-storybook",
    "ui-components:storybook": "yarn nx run ui-components:storybook",
    "ui-components:build-storybook": "yarn nx run ui-components:build-storybook",
    "images:imagemin": "imagemin 'libs/images/src/lib/assets/*.png' --plugin=pngquant --out-dir=libs/images/optimized",
    "chromatic": "yarn chromatic --project-token=CHROMATIC_PROJECT_TOKEN -d=dist/storybook/ui-components"
  },
