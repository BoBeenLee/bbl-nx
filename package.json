  "scripts": {
    "post:new": "pnpm plop post",
    "components:new": "pnpm plop ui-components",
    "start": "pnpm nx serve",
    "dev": "pnpm nx serve",
    "build": "pnpm nx run-many --target=build --all",
    "build:affected:all": "pnpm nx affected --target=build --all",
    "eslint": "pnpm nx run-many --target=lint --all",
    "eslint:fix": "pnpm nx run-many --target=lint --configuration=fix --all",
    "test": "npm run test:all",
    "test:all": "pnpm nx run-many --target=test --all",
    "test:affected": "pnpm nx affected --target=test --base=master --head=HEAD",
    "test:affected:ci": "pnpm nx affected --target=test",
    "test-storybook": "pnpm test-storybook --url http://localhost:4400/",
    "storybook": "npm run ui-components:storybook",
    "build-storybook": "pnpm nx run-many --target=build-storybook --projects=ui-components,features",
    "build-storybook:github": "STORYBOOK_PREFIX_PATH=/bbl-nx pnpm nx run-many --target=build-storybook --projects=ui-components,features",
    "bbl-nextjs:analyze": "env-cmd -f ./apps/bbl-nextjs/.analyze.env pnpm nx run bbl-nextjs:serve",
    "bbl-nextjs:storybook": "pnpm nx run bbl-nextjs:storybook",
    "bbl-nextjs:build-storybook": "pnpm nx run bbl-nextjs:build-storybook",
    "features:storybook": "pnpm nx run features:storybook",
    "features:build-storybook": "pnpm nx run features:build-storybook",
    "ui-components:storybook": "pnpm nx run ui-components:storybook",
    "ui-components:build-storybook": "pnpm nx run ui-components:build-storybook",
    "images:imagemin": "imagemin 'libs/images/src/lib/assets/*.png' --plugin=pngquant --out-dir=libs/images/optimized",
    "chromatic": "pnpm chromatic --project-token=CHROMATIC_PROJECT_TOKEN -d=dist/storybook/ui-components"
  },
