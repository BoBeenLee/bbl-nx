{
  "presets": [
    "next/babel",
    [
      "@babel/env",
      {
        "targets": {
          "node": 6
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "module-resolver",
      {
        "root": [
          "."
        ],
        "alias": {
          "src": "./src"
        },
        "cwd": "babelrc"
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": false
      }
    ],
    [
      "@babel/plugin-transform-modules-commonjs"
    ]
  ]
}
